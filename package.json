{
  "name": "muzart",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vite build",
    "build:github": "vite build --base /muzart/",
    "dev": "vite",
    "lint": "pnpm run lint:prettier && pnpm run lint:eslint",
    "lint:eslint": "eslint --cache --max-warnings=0 --ext ts,tsx src",
    "lint:eslint:fix": "eslint --max-warnings=0 --ext ts,tsx --fix src",
    "lint:fix": "pnpm run lint:prettier:fix && pnpm run lint:eslint:fix",
    "lint:prettier": "prettier --check \"src/**/*.{js,ts,tsx}\"",
    "lint:prettier:fix": "prettier --write --check \"src/**/*.{js,ts,tsx}\"",
    "preview": "vite preview",
    "tc": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "mobx": "^6.10.2",
    "mobx-react-lite": "^4.0.4",
    "preact": "^10.17.1",
    "react-bootstrap-icons": "^1.10.3",
    "tinykeys": "^2.1.0",
    "wouter": "^2.11.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.5.0",
    "@types/lodash": "^4.14.197",
    "@types/node": "^20.5.8",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "autoprefixer": "^10.4.15",
    "css-loader": "^6.8.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "pnpm": "^8.7.1",
    "postcss": "^8.4.29",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.1.3",
    "prettier": "^3.0.3",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.33.0"
  }
}
