{
  "name": "muzart",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build:github": "vite build --base /muzart/",
    "dev": "vite",
    "lint": "pnpm run lint:prettier && pnpm run lint:eslint",
    "lint:eslint": "eslint --cache --max-warnings=0 --ext ts,tsx .",
    "lint:eslint:fix": "eslint --max-warnings=0 --ext ts,tsx --fix .",
    "lint:fix": "pnpm run lint:prettier:fix && pnpm run lint:eslint:fix",
    "lint:prettier": "prettier --check \"**/*.{js,ts,tsx}\"",
    "lint:prettier:fix": "prettier --write --check \"**/*.{js,ts,tsx}\"",
    "preview": "vite preview",
    "tc": "pnpm typecheck",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.1",
    "clsx": "^2.1.0",
    "emittery": "^1.0.1",
    "inflected": "^2.1.0",
    "lodash": "^4.17.21",
    "mobx": "^6.12.0",
    "mobx-react-lite": "^4.0.5",
    "preact": "^10.19.3",
    "tinykeys": "^2.1.0",
    "wouter": "^2.12.1"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.8.1",
    "@types/audioworklet": "^0.0.53",
    "@types/inflected": "^2.1.3",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.9",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "autoprefixer": "^10.4.17",
    "css-loader": "^6.9.1",
    "csstype": "^3.1.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "pnpm": "^8.15.0",
    "postcss": "^8.4.33",
    "postcss-loader": "^8.0.0",
    "postcss-nesting": "^12.0.2",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.2.4",
    "prettier-plugin-packagejson": "^2.4.9",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.2.2"
  },
  "packageManager": "pnpm@9.0.0-alpha.0+sha256.73ada1a82ae5a62c3da44e4841e56d471a5f52a48590b212ea04466923689e3b",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/core",
        "react",
        "react-dom",
        "webpack"
      ]
    }
  }
}
