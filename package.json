{
  "name": "muzart",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:github": "vite build --base /muzart/",
    "preview": "vite preview",
    "tc": "tsc --noEmit",
    "lint": "yarn run lint:prettier && yarn run lint:eslint",
    "lint:fix": "yarn run lint:prettier:fix && yarn run lint:eslint:fix",
    "lint:prettier": "prettier --check \"src/**/*.{js,ts,tsx}\"",
    "lint:prettier:fix": "prettier --write --check \"src/**/*.{js,ts,tsx}\"",
    "lint:eslint": "eslint --cache --max-warnings=0 --ext ts,tsx src",
    "lint:eslint:fix": "eslint --max-warnings=0 --ext ts,tsx --fix src"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "mobx": "^6.6.0",
    "mobx-react-lite": "^3.4.0",
    "preact": "^10.10.1",
    "react-bootstrap-icons": "^1.9.1",
    "tinykeys": "^1.4.0",
    "wouter": "^2.9.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.4.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "autoprefixer": "^10.4.7",
    "css-loader": "^6.7.1",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-standard": "^5.0.0",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "postcss-preset-env": "^7.6.0",
    "prettier": "^2.6.2",
    "style-loader": "^3.3.1",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.8.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "resolutions": {
    "eslint": "8.26.0"
  }
}
