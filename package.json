{
  "name": "muzart",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vite build",
    "build:github": "vite build --base /muzart/",
    "dev": "vite",
    "lint": "yarn run lint:prettier && yarn run lint:eslint",
    "lint:eslint": "eslint --cache --max-warnings=0 --ext ts,tsx src",
    "lint:eslint:fix": "eslint --max-warnings=0 --ext ts,tsx --fix src",
    "lint:fix": "yarn run lint:prettier:fix && yarn run lint:eslint:fix",
    "lint:prettier": "prettier --check \"src/**/*.{js,ts,tsx}\"",
    "lint:prettier:fix": "prettier --write --check \"src/**/*.{js,ts,tsx}\"",
    "preview": "vite preview",
    "tc": "tsc --noEmit",
    "test": "globstar -- node --loader tsx --test \"test/**/*.test.ts\"",
    "test:watch": "globstar -- node --loader tsx --test --watch \"test/**/*.test.ts\""
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "mobx": "^6.9.0",
    "mobx-react-lite": "^4.0.2",
    "preact": "^10.13.1",
    "react-bootstrap-icons": "^1.10.2",
    "tinykeys": "^1.4.0",
    "wouter": "^2.10.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.5.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.15.10",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "autoprefixer": "^10.4.14",
    "css-loader": "^6.7.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-standard": "^5.0.0",
    "globstar": "^1.0.0",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.1.0",
    "postcss-preset-env": "^8.1.0",
    "prettier": "^2.8.7",
    "style-loader": "^3.3.2",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.6",
    "typescript": "^5.0.2",
    "vite": "^4.2.1"
  },
  "resolutions": {
    "eslint": "8.26.0"
  },
  "volta": {
    "node": "18.14.0",
    "yarn": "3.4.1"
  },
  "packageManager": "yarn@3.4.1"
}
